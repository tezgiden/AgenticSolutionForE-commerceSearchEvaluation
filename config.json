    "autozone": {
      "site_name": "AutoZone",
      "target_url": "https://www.autozone.com/",
      "search_tasks": [
        {"query": "brake pads"},
        {"query": "oil filter"},
        {"query": "air filter"},
        {"query": "spark plugs"},
        {"query": "battery"},
        {"query": "alternator"},
        {"query": "starter"},
        {"query": "radiator"},
        {"query": "transmission fluid"},
        {"query": "brake fluid"}
      ],
      "inventory_test_cases": [
        {"query": "AC Delco", "search_type": "multiple_terms"},
        {"query": "brake pad", "search_type": "english_word"},
        {"query": "honda civic brake pad", "search_type": "multiple_terms"}
      ],
      "scraping_config": {
        "search_input_selectors": [
          "#search-field",
          "input[name=\"searchTerm\"]",
          "input[placeholder*=\"Search\"]",
          "input.search-input"
        ],
        "search_button_selectors": [
          "//button[@aria-label=\"Search\"]",
          "//button[contains(@class, 'search-submit')]",
          "//input[@value=\"Search\"]"
        ],
        "product_card_selectors": [
          "div[data-testid=\"product-card\"]",
          "div.product-tile",
          "article.product-item"
        ],
        "product_link_selector": "a[data-testid=\"product-link\"]",
        "product_title_selectors": ["h3[data-testid=\"product-name\"]", "span.product-title"],
        "product_sku_selectors": ["span[data-testid=\"part-number\"]", "span.sku"],
        "product_price_selectors": ["span[data-testid=\"price\"]", "div.price"],
        "product_quantity_selectors": ["span[data-testid=\"availability\"]", "div.stock-status"],
        "no_results_selectors": [
          "div[data-testid=\"no-results\"]",
          "div.no-products",
          "p.empty-state"
        ],
        "max_results_per_query": 12,
        "wait_timeout": 8,
        "page_load_timeout": 25
      },
      "output_config": {
        "output_file": "autozone_evaluation_results.json",
        "detailed_output_file": "autozone_detailed_analysis.json"
      }
    }
